<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a3levelauthenticationsystem">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Feedback"></activity>
        <activity
            android:name=".Home"
            android:theme="@style/AppTheme" />
        <activity android:name=".Loginfinal" />
        <activity android:name=".Reg3Final" />
        <activity
            android:name=".RegisterActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Loginactivity2"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".LoginActivity3"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".RegisterActivity2"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".RegisterActivity3"
            android:theme="@style/AppTheme" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>